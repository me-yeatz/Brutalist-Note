{
  "version": 2,
  "rewrites": [
    { "source": "/app.html", "destination": "/public/app.html" },
    { "source": "/app", "destination": "/public/app.html" },
    { "source": "/index.html", "destination": "/public/index.html" },
    { "source": "/", "destination": "/public/index.html" },
    { "source": "/(.*)", "destination": "/public/$1" }
  ]
}
